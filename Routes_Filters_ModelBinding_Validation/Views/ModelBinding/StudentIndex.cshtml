@{
    ViewData["Title"] = "Student Page";
}
@model Student;

<div><h2>Student</h2></div>
<div>
    First Name
    @Html.TextBoxFor(m => m.FirstName)
</div>

<div>
    Last Name
    @Html.TextBoxFor(m => m.LastName)
</div>
<div>
    Student Level
    @Html.TextBoxFor(m => m.Grade)
</div>
<div>
    UID
    @Html.TextBoxFor(m => m.UID)
</div>

<input type="button" value="update" name="UPDATE"/>

